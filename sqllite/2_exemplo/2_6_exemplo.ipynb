   "source": [
    "import sqlite3\n",
    "\n",
    "try:\n",
    "    with sqlite3.connect('my.db') as conn:\n",
    "        cur = conn.cursor()\n",
    "        delete_stmt = 'DELETE FROM tasks WHERE id = ?'\n",
    "        cur.execute(delete_stmt, (1,))\n",
    "        conn.commit()\n",
    "except sqlite3.Error as e:\n",
    "    print(e)"
   ]
  }
